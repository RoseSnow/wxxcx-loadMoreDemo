.img-list{
    display: flex;
    justify-content: space-between;
    flex-wrap: wrap;
    margin: 5px;
}

.item{
    width: 48.3%;
    height: 400rpx;
    border: 1px solid #c4c4c4;
    box-shadow: 5px 5px 4px #888888;
    margin: 10rpx 0;
}
 
.item image{
    width: 100%;
    height: 100%;
}


  
  
  
